{
    "name": "vsc-redshift-extension",
    "displayName": "vsc_redshift_extension",
    "description": "A VSC extension to support Redshift (AWS) sql work",
    "version": "0.0.6",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sql",
            "aliases": ["sql", "sql"],
            "extensions": [".sql"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sql",
            "scopeName": "source.sql",
            "path": "./syntaxes/sql.tmLanguage.json"
        }]
    }
}
